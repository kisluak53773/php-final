{
    "name": "dzianis/php-exceptions",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "generate": "./vendor/bin/doctrine-migrations generate",
        "migrate": "./vendor/bin/doctrine-migrations migrate",
        "migrate:first": "./vendor/bin/doctrine-migrations migrate first",
        "migrate:prev": "./vendor/bin/doctrine-migrations migrate prev",
        "migrate:next": "./vendor/bin/doctrine-migrations migrate next",
        "migrate:latest": "./vendor/bin/doctrine-migrations migrate latest",
        "status": "./vendor/bin/doctrine-migrations status",
        "diff": "./vendor/bin/doctrine-migrations diff",
        "generate-cache": "./vendor/bin/doctrine orm:generate-proxies"
    },
    "authors": [
        {
            "name": "dzianis",
            "email": "kisluka53773@gmail.com"
        }
    ],
    "require": {
        "monolog/monolog": "^3.6",
        "psr/http-message": "^2.0",
        "doctrine/dbal": "^4.0",
        "vlucas/phpdotenv": "^5.6",
        "doctrine/orm": "^3.2",
        "symfony/cache": "^7.1",
        "doctrine/migrations": "^3.7",
        "twig/twig": "^3.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
